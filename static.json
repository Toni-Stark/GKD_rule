  "globalGroups": [
    {
      "name": "开屏广告",
      "key": 0,
      "matchTime": 10000,
      "resetMatch": "app",
      "actionMaximumKey": 5,
      "rules": [
        {
          "quickFind": true,
          "key": 0,
          "matches": [
            "[text*=\"跳过\" || desc*=\"跳过\"][text.length<10][visibleToUser=true]"
          ]
        },
        {
          "quickFind": true,
          "key": 1,
          "desc": "穿山甲广告特殊标识符",
          "matches": [
            "[id*=\":id/tt_splash_view\"] >n  [id*=\":id/tt_splash_skip_btn\"]"
          ]
        }
      ]
    },
    {
      "name": "权限设置",
      "key": -1,
      "matchTime": 10000,
      "actionMaximum": 2,
      "resetMatch": "activity",
      "actionCdKey": 0,
      "rules": [
        {
          "quickFind": true,
          "key": 1,
          "matches": [
            "[vid=\"permission_deny_button\" || vid=\"permission_allow_foreground_only_button\" || vid=\"permission_allow_one_time_button\"]"
          ]
        },
        {
          "quickFind": true,
          "key":2,
          "desc": "空间访问权限",
          "matches": [
            "[text*=\"空间权限\"] <<n [id=\"android:id/content\"] >n [text~=\"去授权\"]"
          ]
        }
      ]
    },
    {
      "name": "隐私用户协议弹窗",
      "key": 9,
      "matchTime": 10000,
      "actionMaximum": 3,
      "resetMatch": "app",
      "actionCdKey": 0,
      "actionMaximumKey": 0,
      "order": 1,
      "rules": [
        {
          "quickFind": true,
          "key": 0,
          "matches": [
            "[text*=\"隐私政策\"] ",
            "TextView <n LinearLayout > [text*=\"阅读并同意\"]"
          ]
        },
        {
          "quickFind": true,
          "key": 40,
          "preKeys": [0],
          "matches": [
            "[text*=\"隐私政策\"]",
            "TextView <n LinearLayout > [text*=\"同意\" && text.length<10]"
          ]
        },
        {
          "quickFind": true,
          "key": 1,
          "matches": [
            "[text*=\"隐私声明\"] <2 LinearLayout",
            "LinearLayout >2 [text=\"同意\"]"
          ]
        },
        {
          "quickFind": true,
          "key": 2,
          "matches": [
            "[text*=\"(隐私政策||隐私权政策||隐私条款)\"]",
            "TextView <<n [id=\"android:id/content\"] >n [text*=\"同意\"]"
          ]
        },
        {
          "quickFind": true,
          "key": 3,
          "matches": [
            "[vid=\"agree\" || vid=\"viewAgree\"]"
          ]
        }
      ]
    },
    {
      "name": "局部广告",
      "key": 6,
      "matchTime": 10000,
      "actionMaximum": 2,
      "resetMatch": "app",
      "actionCdKey": 0,
      "actionMaximumKey": 0,
      "order": 1,
      "rules": [
        {
          "quickFind": true,
          "key": 0,
          "matches": [
            "[vid=\"activityImageDraweeView\"] + [vid=\"closeImageView\"]"
          ]
        },
        {
          "quickFind": true,
          "key": 1,
          "matches": [
            "[name=\"android.view.ViewGroup\"] > [vid=\"iv_close\"]"
          ]
        },
        {
          "quickFind": true,
          "key": 2,
          "matches": [
            "[desc=\"弹窗\"] + [desc=\"关闭按钮\"]"
          ]
        },
        {
          "quickFind": true,
          "key": 3,
          "matches": [
            "[text*=\"推荐\" && text.length <= 15] < LinearLayout + [vid=\"iv_close\"]"
          ]
        },
        {
          "quickFind": true,
          "key": 4,
          "matches": [
            "[vid=\"activityImageDraweeView\"] + [vid=\"closeImageView\"]"
          ]
        },
        {
          "quickFind": true,
          "key": 5,
          "matches": [
            "[text*=\"签到\" && text.length <= 15] <<n [id=\"android:id/content\"] >n [vid=\"cancel\"||vid=\"iv_close\"]"
          ]
        },
        {
          "quickFind": true,
          "key": 6,
          "matches": [
            "[text*=\"登录领\"] < ViewGroup - ViewGroup"
          ]
        },
        {
          "quickFind": true,
          "key": 7,
          "matches": [
            "[left>=50][right>=50] >n Image[width<=80][height<=80][left>=600] < View[width<=100][height<=100]"
          ]
        },
        {
          "quickFind": true,
          "key": 8,
          "desc": "穿山甲内部关闭弹窗广告",
          "matches": [
            "[id*=\":id/tt_video_ad_close_layout\"]"
          ]
        },
        {
          "quickFind": true,
          "key": 9,
          "action": "clickCenter",
          "desc": "穿山甲内部跳过弹窗广告",
          "matches": [
            "[left>=50][right>=50] >n ImageView[width<=80][height<=80][left>=600] < RelativeLayout[width<=100][height<=100]"
          ]
        },
        {
          "quickFind": true,
          "key": 10,
          "action": "clickCenter",
          "desc": "大麦类似内部弹窗广告",
          "matches": [
            "[id*=\":id/homepage_popup_window_close_btn\"]"
          ]
        }
      ]
    },
